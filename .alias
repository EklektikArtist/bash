alias cl='clear'
alias cp='sudo cp'
alias find='sudo find'
alias batt='~/bash/battery'
alias fdh='ls | grep $1'
alias fdr='ls -lR | grep -E "$1" >> ./temp && python ~/bash/fdr.py temp && rm temp'
alias ifdown='sudo ifconfig wlan0 down'
alias ifup='sudo ifconfig wlan0 up'
alias install='sudo apt-get install' $*
alias more='less'
alias mv='sudo mv'
alias nano='sudo nano'
alias sa='source ~/.bashrc'
alias scp='sudo scp'
alias shutdown='sudo shutdown now'
alias srch='~/bash/search'
alias tatt='tmux a -t' $*
alias tnew='tmux new -s' $*
alias update='sudo apt-get update && sudo apt-get upgrade'
alias weather='bash ~/bash/Terminal-Weather-Radar-master/weather.sh'

alias batt='~/Home/bash/battery'
alias cl='clear'
alias cp='sudo cp'
alias fdh='ls | grep $1'
alias fdr='ls -lR | grep -E "$1" >> ./temp && python ~/Home/bash/fdr.py temp && rm temp'
alias find='sudo find'
alias ifdown='sudo ifconfig wlan0 down'
alias ifup='sudo ifconfig wlan0 up'
alias install='sudo apt-get install'
alias ispeed='speedtest-cli'
alias more='less'
alias mv='sudo mv'
alias nano='sudo nano'
alias ps='ps --forest'
alias sa='source ~/.bashrc'
alias scp='sudo scp'
alias shutdown='sudo shutdown now'
alias srch='~/Home/bash/search' $*
alias tatt='tmux a -t' $*
alias tnew='tmux new -s' $*
alias update='sudo apt-get update && sudo apt-get upgrade'
alias weather='bash ~/Home/bash/Terminal-Weather-Radar-master/weather.sh'


